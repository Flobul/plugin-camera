{
    "edimax.ic7110w": {
        "name": "Edimax IC7110w",
        "vendor": "Edimax",
        "configuration": {
            "urlStream": "\/mjpg\/video.mjpg",
            "boundary": "--myboundary",
            "dashboardSize": "160x120",
            "mobileSize": "120x90"
        },
        "commands": [
            {
                "name": "Droite",
                "type": "action",
                "subtype": "other",
                "isVisible": 1,
                "configuration": {
                    "request": "\/camera-cgi\/com\/ptz.cgi?move=right"
                },
                "display": {
                    "icon": "<i class=\"fa fa-arrow-right\"><\/i>"
                }
            },
            {
                "name": "Haut",
                "type": "action",
                "subtype": "other",
                "isVisible": 1,
                "configuration": {
                    "request": "\/camera-cgi\/com\/ptz.cgi?move=up"
                },
                "display": {
                    "icon": "<i class=\"fa fa-arrow-up\"><\/i>"
                }
            },
            {
                "name": "Bas",
                "type": "action",
                "subtype": "other",
                "isVisible": 1,
                "configuration": {
                    "request": "\/camera-cgi\/com\/ptz.cgi?move=down"
                },
                "display": {
                    "icon": "<i class=\"fa fa-arrow-down\"><\/i>"
                }
            },
            {
                "name": "Gauche",
                "type": "action",
                "subtype": "other",
                "isVisible": 1,
                "configuration": {
                    "request": "\/camera-cgi\/com\/ptz.cgi?move=left"
                },
                "display": {
                    "icon": "<i class=\"fa fa-arrow-left\"><\/i>"
                }
            },
            {
                "name": "Enregistrer",
                "type": "action",
                "subtype": "other",
                "isVisible": 1,
                "configuration": {
                    "request": 300,
                    "recordCmd": 1
                },
                "display": {
                    "icon": "<i class=\"fa fa-circle\"><\/i>"
                }
            }
        ]
    }
}